    "@sanity/embeddings-index-ui": "^3.0.1",
    "@sanity/icons": "^3.7.4",
    "@sanity/language-filter": "^4.0.6",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.4.0",
    "husky": "^9.1.7",
    "react": "^19.2.3",
    "react-animate-height": "^3.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^18.2.0",
    "sanity": "^5.4.0",
    "sanity-plugin-internationalized-array": "^3.2.2",
