{
  "name": "sanity-assist-test-studio",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "sanity"
  ],
  "license": "UNLICENSED",
  "main": "package.json",
  "scripts": {
    "prebuild": "cd ../plugin && npm run build",
    "build": "sanity build",
    "compile": "tsc --noEmit",
    "deploy": "npm run prebuild && sanity deploy",
    "dev": "concurrently 'cd ../plugin && npm run watch' 'sanity dev --port 3334'",
    "lint": "eslint ./src/**/* --ext .tsx,.ts",
    "start": "sanity start"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@portabletext/types": "^2.0.12",
    "@sanity/assist": "^4.3.2",
    "@sanity/client": "^7.5.0",
    "@sanity/code-input": "^6.0.0",
    "@sanity/document-internationalization": "^4.1.0",
    "@sanity/embeddings-index-ui": "^3.0.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/language-filter": "^4.0.5",
    "@sanity/ui": "^3.0.6",
    "@sanity/vision": "^4.0.0",
    "husky": "^9.1.7",
    "react": "^18.2.0",
    "react-animate-height": "^3.2.3",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sanity": "^4.0.0",
    "sanity-plugin-internationalized-array": "^3.1.4",
    "sanity-plugin-media": "^4.0.0",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "concurrently": "^9.2.0",
    "typescript": "^5.7.2"
  }
}
