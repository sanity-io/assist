{
  "name": "sanity-assist-test-studio",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "sanity"
  ],
  "license": "UNLICENSED",
  "main": "package.json",
  "scripts": {
    "prebuild": "pnpm --filter @sanity/assist build",
    "build": "sanity build",
    "compile": "tsc --noEmit",
